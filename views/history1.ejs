<%- include('partial/header3') -%>
<section id="home">

    <div class="row row1 m-0 ">
        <div class="col-lg-res fm">
            <span class="navbar-brand">
                <img class="imgwidt" src="images/resso.png" alt="">
                <span class="restxt">resso 2.0</span>
            </span>
            <ul class="ul1 ">
                <a href="/YourFM" style="text-decoration:none">
                    <li class="li1 pt-1">
                        <i class="fa-solid fa-satellite-dish fa1"></i>
                        <span class=" restxt1">
                            your fM
                        </span>
                    </li>
                </a>
                <a href="/Browse" style="text-decoration:none">
                    <li class="li1 pt-1 ">
                        <i class=" fa-brands fa-firefox fa1"></i>
                        <span class=" restxt1">
                            browse
                        </span>
                    </li>
                </a>

            </ul>
            <ul class="ul2">
                <li class="li2 restxt1 ">
                    <span class="lib">My Library</span>
                </li>
                <a href="/history" class="text-decoration-none">
                    <li class="li2 restxt1">
                        <span>history</span>
                    </li>

                </a>
                <a href="/settings" class="text-decoration-none">
                    <li class="li2 restxt1">
                        <span>settings</span>
                    </li>
                </a>

            </ul>
        </div>
    <section id="forYou" >
        <div class="foryou  ms-auto ">
            <form action="/song" method="post" class="searching">
                <input
                    type="search"
                    name="songName"
                    class='input1 bg-dark text-light fn'
                    id="tags"
                    placeholder="Search">
                <button type="submit" class="btn bg-dark">
                    <i class="fas fa-search text-white"></i>
                </button>
            </form>
            <h1 class="fortext2">History</h1>
        </div>
        <div class="row rows3 " style="background-color: #121212;" >
            <div class="most clear p-0">
                <h1 class="mostText2">Recently Played</h1>
                <form action="clear" method="get">
                    <button type="submit" class="but">Clear History</button>
                </form>
            </div>
            <div class="col-lg-12 p-2 width">

                <ul id="songs2" class="songs p-0">
                    <% for(i=0;i<array1.length;i++){ %>
                        <li class="trend ajab">
                            <span class="images ajab">
                                <img id="dynamicimage" src=<%= array1[i].image %> class="pngs ajab" alt="">
                            </span>
                            <span class="names ajab">
                                <a id="song" class="RB ajab fm1">
                                    <%= array1[i].musicName %></a>
                                <p id="singer" class="singer ajab fm2">
                                    <%= array1[i].artists %></p>
                            </span>
                        </li>
                    <% } %>

                </ul>
            </div>

        </div>

    </section>

    <%- include('partial/footer3') -%>